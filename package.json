{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "WATCHPACK_POLLING=true WATCHPACK_POLLING_INTERVAL=1000 next dev --hostname 127.0.0.1 --port 3000",
    "dev:turbo": "WATCHPACK_POLLING=true WATCHPACK_POLLING_INTERVAL=1000 next dev --turbo --hostname 127.0.0.1 --port 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "performance": "npm run build && npm run build:analyze",
    "optimize-images": "node scripts/optimize-images.js",
    "lighthouse": "npx lighthouse http://localhost:3000 --output-path ./lighthouse-report.html --save-assets --view"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^16.1.0",
    "@react-three/drei": "^10.7.3",
    "@react-three/fiber": "^9.3.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "critters": "^0.0.23",
    "framer-motion": "^11.15.0",
    "gray-matter": "^4.0.3",
    "ldrs": "^1.1.7",
    "next": "^16.1.0",
    "next-mdx-remote": "^5.0.0",
    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "three": "^0.179.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.17.6",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^16.1.0",
    "postcss": "^8.5.4",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.7.2"
  }
}
